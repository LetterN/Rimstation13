	<!-- Matter bin 1 -->

  <RecipeDef ParentName="AutolatheCrafting">
    <defName>CraftBinA</defName>
    <label>Craft Matter Bin.</label>
    <description>Refine metal into matter bin.</description>
    <workAmount>150</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <MatterBinA>1</MatterBinA>
    </products>
  </RecipeDef>

