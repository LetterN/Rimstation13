<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef Name="CreateCircuitBase" Abstract="True">
	<jobString>Refining materials.</jobString>
    <workAmount>500</workAmount>
	<ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ChemSulphuricAcid</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
		<recipeUsers>
			<li>CircuitImprinter</li>
		</recipeUsers>
  </RecipeDef>
  
  <RecipeDef ParentName="CreateCircuitBase">
    <defName>CreateAutolatheCircuit</defName>
    <label>Create Autolathe circuitry</label>
    <description>Create a circuit board for an Autolathe</description>
    <workAmount>500</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <products>
      <CircuitBoardAutolathe>1</CircuitBoardAutolathe>
    </products>
  </RecipeDef>
 
