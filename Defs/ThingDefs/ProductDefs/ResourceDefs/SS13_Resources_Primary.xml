      <MarketValue>100</MarketValue>
	  <Mass>0.70</Mass>
      <SharpDamageMultiplier>0.3</SharpDamageMultiplier>
      <BluntDamageMultiplier>1.15</BluntDamageMultiplier>
    </statBases>
    <thingCategories>
