<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Note, We dont use console screens anymore, Use glass instead.  ~Letter N
	Cheat sheet 
	Mats name: MatterBinA, MicroManipA, ScanModuleA, CapacitorA, LaserA
	Mats num value: A = T1 Mats, B = T2 Mats, C = T3 Mats, D = T4 Bluespace Mats.  -->

	 <!--=============== T1 ====================-->

		<!-- Matter Bin -->

	<ThingDef ParentName="ResourceBase">
    <defName>MatterBinA</defName>
    <label>matter bin</label>
    <description>A container designed to hold compressed matter awaiting reconstruction.</description>
    <graphicData>
      <texPath>Mats/Matterbin1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <resourceReadoutPriority>First</resourceReadoutPriority>		<!--============ REMEMBER TO DO THE SILVER =================-->
    <soundInteract>Silver_Drop</soundInteract>
    <soundDrop>Silver_Drop</soundDrop>
    <useHitPoints>false</useHitPoints>
    <resourceReadoutAlwaysShow>true</resourceReadoutAlwaysShow>
    <statBases>
	  <MaxHitPoints>150</MaxHitPoints>
      <MarketValue>1</MarketValue>
	  <Mass>0.070</Mass>
      <SharpDamageMultiplier>0</SharpDamageMultiplier>
      <BluntDamageMultiplier>0</BluntDamageMultiplier>
    </statBases>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <stackLimit>50</stackLimit>
    <smallVolume>false</smallVolume>
	<deepCommonality>1</deepCommonality>
    <stuffProps>
      <constructEffect>ConstructMetal</constructEffect>
      <soundImpactStuff>BulletImpactMetal</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
      <statOffsets>
        <Beauty>0</Beauty>
      </statOffsets>
      <statFactors>
        <MaxHitPoints>0.7</MaxHitPoints>
        <Flammability>0.2</Flammability>
        <Beauty>0</Beauty>
        <WorkToMake>1.2</WorkToMake>
      </statFactors>
      <smeltable>true</smeltable>
    </stuffProps>
  </ThingDef>
  
