  <ThingDef ParentName="PlantFoodRawBase">
    <defName>RawFlour</defName>
    <label>flour</label>
    <description>Raw flour, technically edible but not very nutritious and pretty disgusting. Better baked into bread. Lasts a long time.</description>
    <graphicData>
      <texPath>Meals/flour</texPath>
	  <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>1</MarketValue>
	  <Mass>0.4</Mass>
    </statBases>
    <ingestible>
      <preferability>DesperateOnly</preferability>
			<nutrition>0.05</nutrition>
			<ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>200</daysToRotStart>
      </li>
    </comps>
  </ThingDef>
  
