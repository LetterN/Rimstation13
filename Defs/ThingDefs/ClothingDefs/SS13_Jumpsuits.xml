  <!-- ==================================== Jumpsuits =====================================-->
  
  <ThingDef Name="SS13JumpsuitBase" ParentName="SS13ApparelMakeableBase" Abstract="True">
    <statBases>
      <WorkToMake>100</WorkToMake>
      <MaxHitPoints>200</MaxHitPoints>
      <ArmorRating_Sharp>0.01</ArmorRating_Sharp>
	  <ArmorRating_Heat>0.01</ArmorRating_Heat>
	  <Insulation_Cold>-10</Insulation_Cold>
	  <Insulation_Heat>5</Insulation_Heat>
    </statBases>
	<apparel>
      <wornGraphicPath>Clothing/Jumpsuits/Atmospherics_Suit/jsatmos</wornGraphicPath>
      <bodyPartGroups>
        <li>Torso</li>
		<li>Neck</li>
		<li>Shoulders</li>
		<li>Arms</li>
		<li>Legs</li>
      </bodyPartGroups>
		<defaultOutfitTags>
			<li>Crew</li>
		</defaultOutfitTags>
    </apparel>
  </ThingDef>

  <ThingDef ParentName="SS13JumpsuitBase">
    <defName>ApparelAtmosJump</defName>
    <label>Atmospherics Jumpsuit</label>
    <description>An Atmospherics Jumpsuit. For when you want to declare independence and hold the station hostage. ALL HAIL THE SOVEREIGN NATION OF ATMOSIA!</description>
    <graphicData>
      <texPath>Clothing/Jumpsuits/Atmospherics_Suit/jsatmos</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costList>
      <Cloth>100</Cloth>
    </costList>
	<thingCategories>
			<li>Apparel</li>
	</thingCategories>
    <equippedStatOffsets>
      <MoveSpeed>+0.05</MoveSpeed>
	  <WorkSpeedGlobal>+0.10</WorkSpeedGlobal>
    </equippedStatOffsets>
    <apparel>
      <wornGraphicPath>Clothing/Jumpsuits/Atmospherics_Suit/jsatmos</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
	  <tags>
			<li>NanoTrasen</li>
	  </tags>
    </apparel>
  </ThingDef>
	

	
