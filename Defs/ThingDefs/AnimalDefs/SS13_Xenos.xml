<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingNaturalBase">
    <defName>XenoNest</defName>
    <label>Xeno Hive</label>
    <description>A hive of Xenomorphs. Will spawn additional Xenomorphs and Eggs over time.</description>
    <thingClass>Hive</thingClass>
    <graphicData>
      <texPath>Creatures/Xeno/aliennest</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.6</drawSize>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <rotatable>false</rotatable>
    <fillPercent>0.60</fillPercent>
    <statBases>
      <MaxHitPoints>230</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <tickerType>Rare</tickerType>
    <passability>PassThroughOnly</passability>
    <comps>
      <li Class="CompProperties_SpawnerHives" />
      <li Class="CompProperties_Spawner">
        <thingToSpawn>AlienWeedsNode</thingToSpawn>
        <spawnCount>1</spawnCount>
        <spawnIntervalRange>
          <min>10000</min>
          <max>15000</max>
        </spawnIntervalRange>
        <spawnMaxAdjacent>3</spawnMaxAdjacent>
      </li>
      <li Class="CompProperties_HeatPusher">
        <heatPerSecond>7</heatPerSecond>
        <heatPushMaxTemperature>38</heatPushMaxTemperature>
      </li>
      <li Class="CompProperties_TemperatureDamaged">
        <safeTemperatureRange>
          <min>-20</min>
          <max>9999</max>
        </safeTemperatureRange>
        <damagePerTickRare>8</damagePerTickRare>
      </li>
      <li Class="CompProperties_SpawnerFilth">
        <filthDef>XenoFilth</filthDef>
        <spawnCountOnSpawn>20</spawnCountOnSpawn>
        <spawnMtbHours>0.1</spawnMtbHours>
        <spawnRadius>7</spawnRadius>
      </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="BuildingNaturalBase">
    <defName>AlienWeedsNode</defName>
    <label>Xeno Node</label>
    <description>A bioluminescent pod produced by Xenomorph nests.</description>
    <thingClass>Building</thingClass>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <selectable>true</selectable>
    <fillPercent>0.20</fillPercent>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <rotatable>false</rotatable>
    <tickerType>Rare</tickerType>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Mass>4</Mass>
      <WorkToBuild>170</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <comps>
      <li Class="CompProperties_Glower">
        <glowRadius>2</glowRadius>
        <glowColor>(77,204,218,25)</glowColor>
      </li>
    </comps>
    <graphicData>
      <texPath>Creatures/Xeno/weedsnode</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
        <offset>(0,0,-0.1)</offset>
      </shadowData>
      <damageData>
        <rect>(0.2,0.2,0.6,0.6)</rect>
      </damageData>
    </graphicData>
  </ThingDef>

