  <ThingDef ParentName="BuildingBase">
    <defName>Morgue</defName>
    <label>morgue</label>
	<designationCategory>Misc</designationCategory>
    <thingClass>Building_Sarcophagus</thingClass>
    <graphicData>
      <texPath>Furniture/Morgue/Empty/morgueempty</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,1)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <description>The Morgue, haven of Changeling CMO's everywhere. Doubles as Assistant sleeping quarters.</description>
    <size>(1,1)</size>
    <pathCost>70</pathCost>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.5</fillPercent>
    <tickerType>Rare</tickerType>
	<canOverlapZones>false</canOverlapZones>
    <building>
      <ai_chillDestination>false</ai_chillDestination>
	  <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <fullGraveGraphicData>
				<texPath>Furniture/Morgue/Full/morguefull</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
				<drawSize>(1,1)</drawSize>
			</fullGraveGraphicData>
      <fixedStorageSettings>
        <filter>
          <categories>
            <li>Corpses</li>
          </categories>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <filter>
          <categories>
            <li>CorpsesHumanlike</li>
          </categories>
          <specialFiltersToDisallow>
            <li>AllowCorpsesStranger</li>
          </specialFiltersToDisallow>
        </filter>
      </defaultStorageSettings>
    </building>
    <statBases>
      <WorkToBuild>2500</WorkToBuild>
      <MaxHitPoints>250</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <inspectorTabs>
      <li>ITab_Storage</li>
      <li>ITab_Art</li>
    </inspectorTabs>
    <comps>
      <li>
        <compClass>CompQuality</compClass>
      </li>
      <li Class="CompProperties_Art">
        <nameMaker>ArtName_SarcophagusPlate</nameMaker>
        <descriptionMaker>ArtDescription_SarcophagusPlate</descriptionMaker>
        <mustBeFullGrave>true</mustBeFullGrave>
      </li>
    </comps>
	    <costList>
      <Steel>100</Steel>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
	<blockWind>true</blockWind>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <staticSunShadowHeight>0.3</staticSunShadowHeight>
  </ThingDef>
  
