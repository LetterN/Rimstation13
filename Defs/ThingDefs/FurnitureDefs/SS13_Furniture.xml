  <ThingDef ParentName="FurnitureBase">
    <defName>SurgicalTable</defName>
    <label>surgical table</label>
    <description>A surgical table, the perfect place to remove the glass from the chaplains feet for the third time this shift. </description>
    <thingClass>Building_Bed</thingClass>
    <graphicData>
      <texPath>Furniture/optable</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1,1)</drawSize>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>2850</WorkToBuild>
      <Flammability>1.0</Flammability>
      <BedRestEffectiveness>1</BedRestEffectiveness>
      <Comfort>0.80</Comfort>
      <ImmunityGainSpeedFactor>1.3</ImmunityGainSpeedFactor>
      <MedicalTendQualityOffset>1.2</MedicalTendQualityOffset>
    </statBases>
    <size>(1,1)</size>
    <costList>
      <Steel>100</Steel>
      <Medicine>10</Medicine>
    </costList>
    <building>
      <bed_showSleeperBody>True</bed_showSleeperBody>
      <bed_healPerDay>25</bed_healPerDay>
      <bed_defaultMedical>true</bed_defaultMedical>
    </building>
	<rotatable>false</rotatable>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>VitalsMonitor</li>
        </linkableFacilities>
      </li>
			<li Class="CompProperties_RoomIdentifier">
				<roomStat>Impressiveness</roomStat>
			</li>
    </comps>
    <fillPercent>0.4</fillPercent>
    <pathCost>60</pathCost>
    <leaveResourcesWhenKilled>True</leaveResourcesWhenKilled>
    <staticSunShadowHeight>0.17</staticSunShadowHeight>
    <castEdgeShadows>True</castEdgeShadows>
    <passability>PassThroughOnly</passability>
    <drawGUIOverlay>True</drawGUIOverlay>
	<canOverlapZones>false</canOverlapZones>
    <designationCategory>Misc</designationCategory>
	
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
  </ThingDef>

