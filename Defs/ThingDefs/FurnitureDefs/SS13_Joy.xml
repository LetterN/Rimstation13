  <ThingDef ParentName="BuildingBase">
    <defName>ArcadeMachine</defName>
    <label>arcade machine</label>
    <description>A way to spend your downtime when you should be setting up the emitters. Someone else will get it.</description>
    <graphicData>
      <texPath>Misc/Arcade/arcade</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shadowData>
        <volume>(0.7, 0.6, 0.7)</volume>
        <offset>(0,0,-0.15)</offset>
      </shadowData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <rotatable>true</rotatable>
    <minifiedDef>MinifiedFurniture</minifiedDef>
    <socialPropernessMatters>true</socialPropernessMatters>
    <fillPercent>0.40</fillPercent>
    <statBases>
      <WorkToMake>1</WorkToMake>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <MarketValue>700</MarketValue>
      <EntertainmentStrengthFactor>1</EntertainmentStrengthFactor>
    </statBases>
    <costList>
      <Steel>100</Steel>
      <Component>8</Component>
    </costList>
    <pathCost>70</pathCost>
    <passability>PassThroughOnly</passability>
    <building>
      <spawnedConceptLearnOpportunity>TVForSickPeople</spawnedConceptLearnOpportunity>
      <boughtConceptLearnOpportunity>TVForSickPeople</boughtConceptLearnOpportunity>
      <watchBuildingStandDistanceRange>
        <min>1</min>
        <max>3</max>
      </watchBuildingStandDistanceRange>
			<watchBuildingStandRectWidth>4</watchBuildingStandRectWidth>
    </building>
    <placeWorkers>
      <li>PlaceWorker_WatchArea</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <startElectricalFires>true</startElectricalFires>
        <basePowerConsumption>200</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
    </comps>
	
    <tradeTags>
      <li>Television</li>
    </tradeTags>
    <constructEffect>ConstructMetal</constructEffect>
    <designationCategory>Joy</designationCategory>
  </ThingDef>
  
