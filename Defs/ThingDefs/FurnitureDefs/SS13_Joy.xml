<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="BuildingBase">
    <defName>SnackVendor</defName>
    <label>snack vendor</label>
    <description>Filled with a seemingly infinite amount of delicious yet nutritionless calories.(Gives Joy not food).</description>
    <graphicData>
      <texPath>Furniture/Vendors/snack</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>
      <damageData>
        <rect>(0.09375,0.296875,0.796875,0.703125)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <minifiedDef>MinifiedFurniture</minifiedDef>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <rotatable>false</rotatable>
    <socialPropernessMatters>true</socialPropernessMatters>
    <fillPercent>0.40</fillPercent>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>1</WorkToMake>
      <Flammability>1.0</Flammability>
      <Beauty>10</Beauty>
      <EntertainmentStrengthFactor>1</EntertainmentStrengthFactor>
    </statBases>
    <comps>
      <li>
        <compClass>CompQuality</compClass>
      </li>
    </comps>
	
    <costList>
      <Steel>25</Steel>
    </costList>
    <pathCost>70</pathCost>
    <passability>PassThroughOnly</passability>
    <designationCategory>Joy</designationCategory>
  </ThingDef>
  
