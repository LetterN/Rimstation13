  </BiomeDef>

  <BiomeDef ParentName="BiomeSpaceParent">
	<defName>Ocean</defName>
  </BiomeDef>

  <BiomeDef ParentName="BiomeSpaceParent">
	<defName>Lake</defName>
  </BiomeDef>
  
